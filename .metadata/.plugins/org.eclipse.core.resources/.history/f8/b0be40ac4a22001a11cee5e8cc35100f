public class AcheErro {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double i = 50.0;
		double k = i + 50.0;
		double j = k + 1;
		
		/* erro bobo. Nao pode pular string assim sem concatenar
		 System.out.println("j is "+j+" and "
				 "k is "+k);
				 
		*/
		
		System.out.println("j is "+j+" and " +
				 "k is "+k);
		

	}

}
